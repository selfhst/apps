    <author>
      <name>homarr-labs/homarr</name>
    </author>
    <content>&lt;h2&gt;&lt;a href="https://github.com/homarr-labs/homarr/compare/v1.30.1...v1.31.0"&gt;1.31.0&lt;/a&gt; (2025-08-01)&lt;/h2&gt;
&lt;h3&gt;Features&lt;/h3&gt;
