  <entry>
    <id>https://github.com/n8n-io/n8n/releases/tag/n8n%401.105.1</id>
    <title>New release for n8n: n8n@1.105.1</title>
    <updated>2025-08-01T08:06:20-04:00</updated>
    <author>
